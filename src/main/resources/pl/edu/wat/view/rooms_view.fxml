<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.AnchorPane"
         xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="pl.edu.wat.controllers.RoomsViewController">
    <TableView AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0"
               fx:id="roomsTable">
        <columns>
            <TableColumn text="%rooms.table.col1" fx:id="numberColumn"/>
            <TableColumn text="%rooms.table.col2" fx:id="numberOfPersonsColumn"/>
            <TableColumn text="%rooms.table.col3" fx:id="numberOfBedsColumn"/>
            <TableColumn text="%rooms.table.col4" fx:id="priceColumn"/>
        </columns>
    </TableView>
</fx:root>